@import "/utils/common.wxss";
page {
  line-height: 1.6;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
  height: 100%;
}
.page{
  background-color: #e7e7e7;
}
.i-header{
  background-image: url(data:image/png;base64,);
  text-align: center;
  background-size:100% 100%;
  display: inline-block;
  width: 100%;
  height: 350rpx;
}
.infodisplay{
  height: 120rpx;
  width: 94%;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  /*flex-direction: row;*/
  flex-direction: column;
  flex-wrap: wrap;
  border-radius: 24rpx;
  margin-top: -60rpx;
}
.infodisplay-top{
  height: 60%;
  width: 100%;
  display: flex;
  flex-direction: row;
  line-height: 72rpx;
  text-align: center;
}
.infodisplay-top-chinese{
  font-size: 26rpx;
}
.infodisplay-top .equip{
   color: #009ee8;
 }
.infodisplay-top .alarm{
  color: #df1334;
}
.infodisplay-top-num{
  font-size: 52rpx;
  font-weight:bold;
}
.infodisplay-bottom{
  width: 100%;
  height: 40%;
  color: #afafaf;
  display: flex;
  flex-direction: row;
  font-size: 26rpx;
  text-align: center;
  line-height: 48rpx;
}
.my-list{
  height: auto;
  width: 94%;
  background-color: #fff;
  border-radius: 24rpx;
  margin-top: 20rpx;
  font-size: 32rpx;
}
.key-processing{
  width: 100%;
  height: 200rpx;
  border-radius: 24rpx;
}
.key-processing-word{
  width: 100%;
  height: 70rpx;
  display: flex;
  align-items: center;
}
.key-processing text{
  font-size: 28rpx;
  margin-left: 30rpx;
}
.key-processing-cicke{
  width: 100%;
  height: 130rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.one {
  width: 80rpx;
  height: 80rpx;
  background-color: #df1334;
  border-radius: 50%;
  font-size: 22rpx;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
}

.page__bd{
  background-color: #e7e7e7;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
}
.g-bda-userinfo {
  padding-top: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.navigator-hover button {
  background-color: #dedede;
}

.other-navigator-hover button {
  background-color: #dedede;
}



.userinfo-avatar {
  width: 180rpx;
  height: 180rpx;
  margin: 20rpx;
  margin-bottom: 2rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
}

.userinfo-nickname{
  color: #fff;
}

.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
}

.clearData{
  height: 80rpx;
  width: 94%;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  border-radius: 24rpx;
  z-index: 9999;
  flex-wrap: wrap;
  position: fixed;
  bottom: 100rpx;
  font-size: 28rpx;
  color: #e12140;
}
.cancle{
  height: 80rpx;
  width: 94%;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  border-radius: 24rpx;
  z-index: 9999;
  flex-wrap: wrap;
  position: fixed;
  bottom: 10rpx;
  font-size: 28rpx;
}
.modalDlg{
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
}
.cache text{
  font-size: 26rpx;
  color: #7b7b7b;
  margin-right: 40rpx;
}
.weui-cell_primary{
  display: flex;
  align-items: center;
}
.selectcompany{
  width: 30%;
  float: left;
}
.account-equip{
  width: 70%;
  float: left;
  font-size: 24rpx;
  color: #009ee8;
}
.radio-group{
  width: 90%;
  font-size: 26rpx;
}
.checkbox-group{
  width: 80%;
  font-size: 26rpx;
}


.detailpic{
  width: 90%;
  min-height: 200rpx;
  height: 300rpx;
  background-color: white;
  border-radius: 26rpx;
  position: fixed;
  top: 400rpx;
  z-index: 9998;
}

.detailpic-T{
  width: 100%;
  height: 70%;
  background-color: white;
  border-top-left-radius: 24rpx;
  border-top-right-radius: 24rpx;
}

.detailpic-T-text{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-weight: bold;
  height: 80rpx;
}
.detailpic-T-radio{
  justify-content: center;
  align-items: center;
}
.keyalarmone{
  float: left;
  display: flex;
  justify-content: center;
  align-items: center;
  width: auto;
  height: 60rpx;
  border-radius: 12rpx;
  border: 1px solid #e7e7e7;
  margin-left: 50rpx;
  margin-right: 50rpx;
  font-size: 26rpx;
  margin-top: 20rpx;
}
.keyalarmone text{
  font-size: 26rpx;
  margin-top: -35rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.detailpic-B{
  width: 100%;
  height: 30%;
  border-top: 1px solid #e7e7e7;
  background-color: white;
  border-bottom-right-radius: 24rpx;
  border-bottom-left-radius: 24rpx;
  display: flex;
}
.detailpic-BL{
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}
.detailpic-BR{
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  color: #5387c2;
}

.detailpic-T-image{
  width: 80%;
  height: 90%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
}
.detailpic-T-image image{
  width: 100%;
  height: 100%;
  z-index: 9999;
}
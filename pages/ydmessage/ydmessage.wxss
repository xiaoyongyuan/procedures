
page{
    display: flex;
    flex-direction: column;
    height: 100%;
    background-color: #e7e7e7;
}
.page{
    background-color: #e7e7e7;
}
.ydmessagelist{
    width: 100%;
    height: 220rpx;
    background-color: white;
    margin-top: 1px;
}
.ydmessagelist-one{
    width: 100%;
    height: auto;
    background-color: white;
    margin-top: 1px;
}
.ydmessagelist-T{
    width: 100%;
    height: 140rpx;
    display: flex;
    align-items: center;
}
.ydmessagelist-b{
    width: 100%;
    height: 80rpx;
    font-size: 24rpx;
    color: #585858;
    display: flex;
    align-items: center;
}
.ydmessagelist-b .textview{
    margin-left: 20rpx;
    display: flex;
    align-items: center;
    margin-right: 20rpx;
}
.ydmessagelist-T-icon{
    width: 90rpx;
    height: 90rpx;
    margin-left: 20rpx;
    position: relative;
}
.ydmessagelist-T-icon image{
    width: 100%;
    height: 100%;
}
.ydmessagelist-T-center{
    width: 75%;
    height: 90rpx;
    margin-left: 20rpx;
}
.ydmessagelist-T-right{
    width: 5%;
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.ydmessagelist-T-center-t{
    width: 100%;
    height: 60%;
    font-size: 26rpx;
    font-weight: bold;
    display: flex;
    align-items: center;
}
.ydmessagelist-T-center-b{
    width: 100%;
    height: 40%;
    font-size: 22rpx;
    color: #aaa;
}
.read{
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background-color: #df1737;
    position: absolute;
    top: -10rpx;
    right: -10rpx;
}
.up{
    width: 30rpx;
    height: 20rpx;
    background-color: #237af5;
}

.ydmessagelist-T-right image{
    width: 25rpx;
    height: 25rpx;
}
.upimage{
    width: 25rpx;
    height: 25rpx;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
}
.nomessage{
    color: #707070;
    font-size: 26rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 50%;
}
.weui-loadmore {
    width: 100%;
    height: 50rpx;
    font-size: 30rpx;
    text-align: center;
    margin-top: -90rpx;
}
.weui-loading {
    width: 50rpx;
    height: 50rpx;
    display: inline-block;
    vertical-align: middle;
    -webkit-animation: weuiLoading 1s steps(12, end) infinite;
    animation: weuiLoading 1s steps(12, end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
}
.weui-loadmore-tips {
    display: inline-block;
    vertical-align: middle;
}
.loading{
    padding: 10rpx;
    text-align: center;
}
.loading:before{
    display: inline-block;
    margin-right: 5rpx;
    vertical-align: middle;
    content: '';
    width: 40rpx;
    height: 40rpx;

    background-size: contain;
    animation: rotate 1s linear infinite;
}
.loading.complete:before{
    display: none;
}

@import "../ydmessage/ydmessage.wxss";
page{
    line-height: 1.6;
    font-family: -apple-system-font, "Helvetica Neue", sans-serif;
    height: 100%;
}
.page{
    height: 100%;
    background-color: #e7e7e7;
}
.zddkmessagelist{
    height: 500rpx;
    width: 100%;
    background-color: white;
    margin-top: 1px;
}
.zddkmessagelist-one{
    height: 140rpx;
    width: 100%;
    background-color: white;
    margin-top: 1px;
}
.zddkmessagelist-B{
    width: 100%;
    height: 360rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.zddkmessagelist-B-inner{
    width: 94%;
    height: 90%;
    border-radius: 24rpx;
}
.zddkmessagelist-B-inner image{
    width: 100%;
    height: 100%;
    border-radius: 24rpx;
}